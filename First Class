#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
  
  int X;
  float Y;
  char SEXO;
  char NOME[40];

X = 4;
Y = 7.5;
SEXO ='F';
strcpy(NOME, "Atribua antes o nome à string");

 printf("Minha variável X é: %d\n", X);
 printf("Minha variável Y é: %f\n", Y);
 printf("Minha variável Y é: %c\n", SEXO);

 printf("Minha variável NOME é: %s\n", NOME);




  return 0;
}
