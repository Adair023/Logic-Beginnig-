printf("Digite a base:  ");
scanf("%d", &b);


//  Cáculo da área
a = b * h;
printf("A área deste retangulo é: %d\n", a); 
// IMPRIMA A FORMATAÇÃO

 //FIM ALGORÍTIMO

  // %d == NÚMEROS INTEIROS ""
